{
    "manifest_version": 2,
    "version": "1.3",
    "name": "__MSG_MANIFEST_NAME__",
    "short_name": "SearXNG",
    "author": "ndsvw",
    "default_locale": "en",
    "description": "__MSG_MANIFEST_DESCRIPTION__",
    "icons": {
        "16": "images/searxng/searxng-firefox.svg",
        "128": "images/searxng/searxng-firefox.svg"
    },
    "browser_action": {
        "default_title": "SearXNG",
        "default_area": "navbar",
        "default_icon": {
            "16": "images/searxng/searxng-firefox.svg",
            "128": "images/searxng/searxng-firefox.svg"
        }
    },
    "background": {
        "scripts": [
          "background.js"
        ]
      },
    "browser_specific_settings": {
        "gecko": {
            "strict_min_version": "79.0"
        }
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "SearXNG",
            "keyword": "searx, searxng",
            "search_url": "https://{{INSTANCE.DOMAINNAME}}/search?q={searchTerms}",
            "favicon_url": "images/searxng/searxng.svg",
            "suggest_url": "https://{{INSTANCE.DOMAINNAME}}/autocompleter?q={searchTerms}",
            "encoding": "UTF-8",
            "is_default": true
        }
    }
}